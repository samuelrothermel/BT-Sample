{
  "name": "braintree-hosted-fields-sample",
  "version": "1.0.0",
  "description": "A simple Braintree Hosted Fields integration with Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "report": "node generate-report.js",
    "report:daily": "node generate-report.js --days 1 --format table",
    "report:weekly": "node generate-report.js --days 7 --format csv",
    "report:monthly": "node generate-report.js --days 30 --format csv"
  },
  "dependencies": {
    "braintree": "^3.32.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
